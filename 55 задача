a,b=input(),input()
if (a=='красный' and b=='синий') or (b=='красный' and a=='синий'):
    print('фиолетовый')
elif (a=='красный' and b=='желтый') or (a=='желтый' and b=='красный'):
    print('оранжевый')
elif (a=='синий' and b=='желтый') or (a=='желтый' and b=='синий'):
    print('зеленый')
elif a==b:
    if a!='красный' and a!='синий' and a!='желтый':
        print('ошибка цвета')
    else:
        print(a)
else:
    print('ошибка цвета')
