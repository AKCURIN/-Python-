a=int(input())
b=int(input())
c=str(input())
if c!='+' and c!='-' and c!='*' and c!='/':
    print("Неверная операция")
else:    
    if c=='/':
        if b==0:
            print("На ноль делить нельзя!")
        else:
            print(a/b)
    elif c=='+':
        print(a+b)
    elif c=='-':
        print(a-b)
    else:
        print(a*b)
